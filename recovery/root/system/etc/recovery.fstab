# mount point  fstype  device                             flags
/system       ext4    /dev/block/mapper/system           flags=display="System";backup=1
/vendor       ext4    /dev/block/mapper/vendor           flags=display="Vendor";backup=1;wipeingui
/product      ext4    /dev/block/mapper/product          flags=display="Product";backup=1;wipeingui
/data         f2fs    /dev/block/bootdevice/by-name/userdata  flags=fileencryption=aes-256-xts
/boot         emmc    /dev/block/bootdevice/by-name/boot      flags=backup=1;flashimg=1
/vbmeta       emmc    /dev/block/bootdevice/by-name/vbmeta    flags=backup=1;flashimg=1
